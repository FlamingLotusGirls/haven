[Unit]
Description=FLG Haven Flame Controller Webserver
After=network.target

[Service]
Environment="PYTHONPATH=/home/flaming/haven/util"
ExecStart=/usr/bin/python3 /home/flaming/haven/Fire/fire_control/flames_webserver.py
WorkingDirectory=/home/flaming/haven/Fire/fire_control
Restart=always
User=flaming

[Install]
WantedBy=multi-user.target
